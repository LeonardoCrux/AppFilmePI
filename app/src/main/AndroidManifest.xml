<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pi.appfilme">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.FavoritosActivity"></activity>
        <activity android:name=".view.activity.SeasonDetalheActivity" />
        <activity android:name=".view.activity.SerieDetalheActivity" />
        <activity android:name=".view.activity.FotoDetalheActivity" />
        <activity android:name=".view.activity.PessoaDetalheActivity" />
        <activity android:name=".view.activity.ListaExpandidaActivity" />
        <activity android:name=".view.Teste" />
        <activity android:name=".view.activity.ResultadoBuscaActivity" />
        <activity android:name=".view.activity.FilmeDetalheActivity" />
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>